        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <properties url="file:///c:/ab-project/policy/src/abjr-project/config/abjr-policy/local/db.properties" />

    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />

        <!-- 通用Mapper配置 -->
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper" />
        </plugin>

        <jdbcConnection driverClass="${jdbc.driver}" connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.abjr.policy.user.model" targetProject="${targetJavaProject}" />

        <sqlMapGenerator targetPackage="com.abjr.policy.user.mapper" targetProject="${targetResourcesProject}" />

        <javaClientGenerator targetPackage="com.abjr.policy.user.dao" targetProject="${targetJavaProject}" type="XMLMAPPER" />

